<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.monkeygang.mindfactorybooking.Controller.CateringController">
   <children>
      <AnchorPane fx:id="container" prefHeight="450.0" prefWidth="400.0" style="-fx-background-color: white;">
         <children>
            <Text layoutX="110.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Forplejning">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Button fx:id="nextButton" layoutX="332.0" layoutY="410.0" mnemonicParsing="false" onAction="#onNextButtonClicked" text="Next" />
            <RadioButton fx:id="halfDayCoffeeMeetingRadioButton" layoutX="110.0" layoutY="87.0" mnemonicParsing="false" onAction="#radioButtonSelected" text="Halvdags kaffemøde">
               <font>
                  <Font size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="halfDayMeetingRadioButton" layoutX="112.0" layoutY="131.0" mnemonicParsing="false" onAction="#radioButtonSelected" text="Halvdagsmødepakke">
               <font>
                  <Font size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="wholeDayMeetingRadioButton" layoutX="112.0" layoutY="174.0" mnemonicParsing="false" onAction="#radioButtonSelected" text="Heldagsmødepakke">
               <font>
                  <Font size="16.0" />
               </font>
            </RadioButton>
            <Button fx:id="backButton" layoutX="14.0" layoutY="410.0" mnemonicParsing="false" onAction="#onBackButtonClicked" text="Back" />
            <RadioButton fx:id="noCateringRadioButton" layoutX="112.0" layoutY="225.0" mnemonicParsing="false" onAction="#radioButtonSelected" text="ingen forplejning">
               <font>
                  <Font size="16.0" />
               </font>
            </RadioButton>
         </children>
      </AnchorPane>
   </children>
</StackPane>
