<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.monkeygang.mindfactorybooking.Controller.BookingController">
   <children>
      <AnchorPane fx:id="container" prefHeight="450.0" prefWidth="400.0" style="-fx-background-color: white;">
         <children>
            <Label layoutX="133.0" layoutY="14.0" text="Booking">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button fx:id="nextButton" layoutX="342.0" layoutY="262.0" mnemonicParsing="false" onAction="#onNextButtonClick" text="next" />
            <Button fx:id="cancelButton" layoutX="282.0" layoutY="262.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="cancel" />
            <ComboBox fx:id="startTimeCombobox" layoutX="69.0" layoutY="132.0" prefHeight="26.0" prefWidth="78.0" />
            <ComboBox fx:id="endTimeCombobox" layoutX="258.0" layoutY="132.0" prefHeight="26.0" prefWidth="78.0" />
            <Button fx:id="deleteButton" layoutX="225.0" layoutY="262.0" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" />
            <DatePicker fx:id="startDatePicker" layoutX="21.0" layoutY="98.0" />
            <DatePicker fx:id="endDatePicker" layoutX="209.0" layoutY="98.0" />
            <Label layoutX="95.0" layoutY="68.0" text="Fra:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="287.0" layoutY="68.0" text="Til:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="14.0" layoutY="262.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="back" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
