            <Label layoutX="115.0" layoutY="14.0" prefHeight="54.0" prefWidth="170.0" text="Færdiggør">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button fx:id="doneButton" layoutX="330.0" layoutY="260.0" mnemonicParsing="false" onAction="#onDoneButtonClick" text="Bekræft" />
