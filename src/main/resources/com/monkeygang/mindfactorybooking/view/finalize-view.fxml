            <Button fx:id="backButton" layoutX="17.0" layoutY="260.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Tilbage" />
            <TextArea fx:id="finalizeTextArea" layoutX="60.0" layoutY="68.0" prefHeight="177.0" prefWidth="280.0" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
