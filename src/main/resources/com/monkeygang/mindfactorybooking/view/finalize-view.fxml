<StackPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.monkeygang.mindfactorybooking.Controller.FinalizeController">
   <children>
      <AnchorPane fx:id="container" prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: white;">
